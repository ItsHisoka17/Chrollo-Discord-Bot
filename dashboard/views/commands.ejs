}
.navbar {
    overflow: hidden;
    background-color: #eb347a;
}
.navbar a {
    float: left;
    text-decoration: none;
    font-family: Varela Round;
    font-size: 16px;
    padding: 14px 17px;
    text-align: center;
    color: white;
}
.navbar a:hover {
    background-color: #f7c3eb;
    transition: 0.5s;
}
.table {
    font-family: arial, sans-serif;
    border-collapse: collapse;
    width: 100%;
  }
  
  td, th {
    border: 1px solid #dddddd;
    text-align: left;
    padding: 8px;
    font-family: Varela Round;
    font-weight: 450;
  }
  
  tr:nth-child(even) {
    background-color: #faedef
  }
  tr:hover {
      background-color: #fcc0c9;
      transition: 0.5s;
  }
  .dropdown {
  float: left;
  overflow: hidden;
}




.dropdown-content {
  display: none;
  position: absolute;
  top: 5%;
  background-color: #f9f9f9;
  min-width: 160px;
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
  z-index: 1;
}

.dropdown-content a {
  float: none;
  color: black;
  padding: 12px 16px;
  text-decoration: none;
  display: block;
  text-align: left;
}

.dropdown-content a:hover {
  background-color: #ddd;
}

.dropdown:hover .dropdown-content {
  display: block;
}
